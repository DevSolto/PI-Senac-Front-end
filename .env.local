# Base URL da API REST utilizada pelo cliente HTTP
NEXT_PUBLIC_API_BASE_URL=https://api.example.com

# Endpoint para conexões Server-Sent Events (SSE)
NEXT_PUBLIC_API_SSE_URL=https://api.example.com/events

# Credenciais compartilhadas para autenticação - utilize apenas uma das opções abaixo
NEXT_PUBLIC_API_KEY=example-api-key
NEXT_PUBLIC_API_JWT=

# Tempo máximo (em milissegundos) para requisições HTTP antes de cancelar
NEXT_PUBLIC_API_TIMEOUT_MS=10000
